<% if #current_user %>
  <h3>Users Index STUB</h3>
<% else %>
  <h3>Landing Page</h3>
    
  <div class="featureex" id="1">
<% end %>
























































<div class="display-users" style="display:none;">
<table>
  <tr>
    <th>User name</th>
    <th>Password hash</th>
    <th>User email</th>
    <th>User bio</th>
    <th>User zipcode</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.user_name %></td>
    <td><%= user.password_hash %></td>
    <td><%= user.user_email %></td>
    <td><%= user.user_bio %></td>
    <td><%= user.user_zipcode %></td>
    <td><%= link_to 'View User', user %></td>
    <td><%= link_to 'Edit User', edit_user_path(user) %></td>
    <td><%= link_to 'Kill User', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
</div>